<body id="page-top">
<%- include navbar.ejs %>
<div id="wrapper">
    <%- include sidebar.ejs %>
    <div class="card-columns">
        <div class="card" style="width:410px">
            <img class="card-img-top" style="height:250px" src="/images/electronics.jpg">
            <div class="card-body">
                <h4 class="card-title">Electronics</h4>
                <p class="card-text">TV/Laptop/Calculator...</p>
                <a href="/categorySearch?category=Electronics" class="btn btn-primary">View All Electronics</a>
            </div>
        </div>
        <div class="card" style="width:410px">
            <img class="card-img-top" style="height:250px" src="/images/books.jpg">
            <div class="card-body">
                <h4 class="card-title">Books</h4>
                <p class="card-text">Novel/Magazine/Textbook...</p>
